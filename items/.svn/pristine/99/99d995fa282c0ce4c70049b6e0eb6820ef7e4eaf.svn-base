		onload = function(){
			//登录中心显示隐藏
			$(".vip").mouseover(function(){
				$(this).css("background","#fff");
				$(".minicenter").css("display","block")
			})
			$(".vip").mouseout(function(){
				$(this).css("background","");
				$(".minicenter").css("display","none")
			})
			//移除top广告
			$("#adcon > span").click(function(){
				$(this).parent().parent().remove();
			})
			//购物车显示隐藏
			$(".buycarcon").mouseover(function(){
				$(".minicarmenu").css("display","block")
			})
			$(".buycarcon").children().mouseout(function(){
				$(".minicarmenu").css("display","none")
			})
			//二级菜单
			$(".menunav ul:first-child").children("li").mouseover(function(){
				$(this).children("div").show().end().find("b").css("color","#0979e9");
				$(this).css("background","#fff").find(".navlink").css("color","#0979e9").end()
				.siblings().css("background","").find(".navlink").css("color","#fff").end();
				$(this).siblings().children("div").hide().parent().find("b").css("color","#fff")
				
			})
			$(".menunav ul:first-child").children("li").mouseout(function(){
				$(this).css("background","").find(".navlink").css("color","#fff");
				$(this).find(".secMenu").hide().parent().find("b").css("color","#fff");
			})
			//banner图轮播,news轮播,产品banner轮播
			var length1,length2,length3,
			Index1 = Index2 = Index3 = Index4 = Index5 = 0,
			interval1,interval2,interval3,interval4,interval5,
			hasStarted1 = hasStarted2 = hasStarted3 = hasStarted4 = hasStarted5 = false,
			t1 = 5500,t2 = 5000;
			length1 = $(".photoList").children().length;
			length2 = $(".expressCon").find("li").length;
			length3 = $("#deveKits .deveKits-banner .bd li").length;
			$("#linker .deveKits-banner .hd li:first").addClass("highlight");
			$("#deveKits .deveKits-banner .hd li:first").addClass("highlight");
			$("#device .deveKits-banner .hd li:first").addClass("highlight");
			$(".pointList li:first").addClass("highlight");
			$(".bannercon").mouseover(function(){
				stop1();
			})
			$(".express").mouseover(function(){
				stop2();
			})
			$("#deveKits .deveKits-banner .f1").mouseover(function(){
				stop3();
			})
			$("#device .deveKits-banner .f1").mouseover(function(){
				stop4();
			})
			$("#linker .deveKits-banner .f1").mouseover(function(){
				stop5();
			})
			$(".bannercon").mouseout(function(){
				start1();
			})
			$(".express").mouseout(function(){
				start2();
			})
			$("#deveKits .deveKits-banner .f1").mouseout(function(){
				start3();
			})
			$("#device .deveKits-banner .f1").mouseout(function(){
				start4();
			})
			$("#linker .deveKits-banner .f1").mouseout(function(){
				start5();
			})
			$(".pointList li").click(function(){//点哪张显示哪张
				var preIndex = $(".pointList li").filter(".highlight").index();
				Index1 = $(this).index();
				play1(preIndex,Index1);
			})
			$("#deveKits .deveKits-banner .hd li").click(function(){
				var preIndex = $("#deveKits .deveKits-banner .hd li").filter(".highlight").index();
				Index3 = $(this).index();
				play3(preIndex,Index3);
			})
			$("#device .deveKits-banner .hd li").click(function(){
				var preIndex = $("#device .deveKits-banner .hd li").filter(".highlight").index();
				Index4 = $(this).index();
				play4(preIndex,Index4);
			})
			$("#linker .deveKits-banner .hd li").click(function(){
				var preIndex = $("#linker .deveKits-banner .hd li").filter(".highlight").index();
				Index5 = $(this).index();
				play5(preIndex,Index5);
			})
			$(".icon-prev").click(function(){//点击显示上一个news
				prev();
			})
			$(".icon-next").click(function(){//点击显示下一个news
				next();
			})
			function start1(){//开始banner图轮播
				if(!hasStarted1){
					hasStarted1 = true;
					interval1 = setInterval(function(){
						var preIndex = Index1;
						Index1 = ++Index1 % length1;
						play1(preIndex,Index1);
					},t1);
				}
			}
			function start2(){//开始news轮播
				if(!hasStarted2){
					hasStarted2 = true;
					interval2 = setInterval(next,t2)
				}
			}
			function start3(){//开发工具banner轮播
				if(!hasStarted3){
					hasStarted3 = true;
					interval3 = setInterval(function(){
						var preIndex = Index3;
						Index3 = ++Index3 % length3;
						play3(preIndex,Index3);
					},t1);
				}
			}
			function start4(){//被动器材banner轮播
				if(!hasStarted4){
					hasStarted4 = true;
					interval4 = setInterval(function(){
						var preIndex = Index4;
						Index4 = ++Index4 % length3;
						play4(preIndex,Index4);
					},t1);
				}
			}
			function start5(){//连接器材banner轮播
				if(!hasStarted5){
					hasStarted5 = true;
					interval5 = setInterval(function(){
						var preIndex = Index5;
						Index5 = ++Index5 % length3;
						play5(preIndex,Index5);
					},t1);
				}
			}
			function prev(){//上一张
				var preIndex = Index2;
				Index2 = (--Index2 + length2) % length2;
				play2(preIndex,Index2);
			}
			function next(){//下一张
				var preIndex = Index2;
				Index2 = ++Index2 % length2;
				play2(preIndex,Index2);
			}
			function play1(preIndex,Index1){//图片淡入淡出，
				$(".photoList").children().eq(preIndex).fadeOut(300)
				.parent().children().eq(Index1).fadeIn(300);
				$(".pointList li").removeClass("highlight");
				$(".pointList li").eq(Index1).addClass("highlight")
			}
			function play2(preIndex,Index2){
				$(".expressCon").find("li").eq(preIndex).fadeOut(300)
				.parent().children().eq(Index2).fadeIn(300);
			}
			function play3(preIndex,Index3){
				$("#deveKits .deveKits-banner .bd li").eq(preIndex).fadeOut(300)
				.parent().children().eq(Index3).fadeIn(300);
				$("#deveKits .deveKits-banner .hd li").removeClass("highlight");
				$("#deveKits .deveKits-banner .hd li").eq(Index3).addClass("highlight")
			}
			function play4(preIndex,Index4){
				$("#device .deveKits-banner .bd li").eq(preIndex).fadeOut(300)
				.parent().children().eq(Index4).fadeIn(300);
				$("#device .deveKits-banner .hd li").removeClass("highlight");
				$("#device .deveKits-banner .hd li").eq(Index4).addClass("highlight")
			}
			function play5(preIndex,Index5){
				$("#linker .deveKits-banner .bd li").eq(preIndex).fadeOut(300)
				.parent().children().eq(Index4).fadeIn(300);
				$("#linker .deveKits-banner .hd li").removeClass("highlight");
				$("#linker .deveKits-banner .hd li").eq(Index5).addClass("highlight")
			}
			function stop1(){//停止banner轮播
				clearInterval(interval1);
				hasStarted1 = false;
			}
			function stop2(){//停止news轮播
				clearInterval(interval2);
				hasStarted2 = false;
			}
			function stop3(){//停止开发工具banner轮播
				clearInterval(interval3);
				hasStarted3 = false;
			}
			function stop4(){//停止被动器材banner轮播
				clearInterval(interval4);
				hasStarted4 = false;
			}
			function stop5(){//停止连接器材banner轮播
				clearInterval(interval5);
				hasStarted5 = false;
			}
			start1();//开始banner轮播
			start2();//开始news轮播
			start3();//开始开发工具banner轮播
			start4();//开始被动器材banner轮播
			start5();//开始连接器材banner轮播
			//无缝轮播
			$("#deveKits ._top span").mouseenter(function(){
				$("#deveKits .develop").stop(false,true);
				$(this).addClass("on").siblings().removeClass("on");
				var $index = $(this).index();
				$("#deveKits .develop").animate({left:$index*-890})
			})
			$("#device ._top span").mouseenter(function(){
				$("#device .develop").stop(false,true);
				$(this).addClass("on").siblings().removeClass("on");
				var $index = $(this).index();
				$("#device .develop").animate({left:$index*-890})
			})
			$("#linker ._top span").mouseenter(function(){
				$("#linker .develop").stop(false,true);
				$(this).addClass("on").siblings().removeClass("on");
				var $index = $(this).index();
				$("#linker .develop").animate({left:$index*-890})
			})
			//
			$(".animicon").mouseenter(function(){
				$(this).animate({right:3},150)
			})
			$(".animicon").mouseleave(function(){
				$(this).animate({right:0},150)
			})
			//
			$(".overseas-right a").mouseenter(function(){
				$(this).css({"border":"1px solid #0979e9","zIndex":"10"})
				.children("div").css("display","block")
			})
			$(".overseas-right a").mouseleave(function(){
				$(this).css({"border":"1px solid #f2f2f2","zIndex":"0"})
				.children("div").css("display","none")
			})
			$("#hot a").mouseenter(function(){
				$(this).css({"border":"1px solid #0979e9","zIndex":"10"})
				.children("div").css("display","block")
			})
			$("#hot a").mouseleave(function(){
				$(this).css({"border":"1px solid #f2f2f2","zIndex":"0"})
				.children("div").css("display","none")
			})
			//验证码
			var oCode = getclassname4ie("code");
			oCode[0].innerHTML = code(4);
			oCode[0].onclick = function(){
				oCode[0].innerHTML = code(4);
			}
			//楼梯
			var _index = 0;
			var isclick = false;
			$("#stairs a").click(function(){
				isclick = true;
				_index = $(this).index();
				var _top = $(".louti"+_index).offset().top;
				$(this).addClass("f60").siblings().removeClass("f60");
				$("body,html").animate({scrollTop:_top-100},500,function(){
					isclick = false;
				})
			})
			$(window).scroll(function(){//楼梯
				if(!isclick){
					if($(document).scrollTop()>=600){
						$("#stairs").show();
						var index = Math.floor($(document).scrollTop()/550);
						$("#stairs a").eq(index-1).addClass("f60").siblings().removeClass("f60")
					}else{
						$("#stairs").hide();
					}
					//回到顶部
					if($(document).scrollTop()>=500){
						$(".bottomtoolbar a").show();
					}else{
						$(".bottomtoolbar a").hide();
					}
					//顶部悬浮
					if($(document).scrollTop()>=120){
						$("#log").css({"position":"fixed","top":"0","zIndex":"1000","height":"92px","paddingTop":"0","borderBottom":"1px solid #ddd"})
					}else{
						$("#log").css({"position":"","zIndex":"","borderBottom":"","height":"102px","paddingTop":"10px","display":"block"})
					}
				}
			})
			$(".bottomtoolbar a").click(function(){
				$("body,html").animate({scrollTop:0},1000)
				console.log("1111");
			})
		}
