/*右侧栏*/
#navtoolbar{
	position: fixed;
	right: 0;
	top: 0;
	height: 100%;
	border-right: 4px solid #666;
	z-index: 1000;
}
#navtoolbar .bottomtoolbar{
	position: fixed;
	right: 0;
	bottom: 100px;
	width: 30px;
	height: 36px;
}
#navtoolbar .bottomtoolbar a{
	display: block;
	width: 34px;
	border-radius: 2px;
	background: #666;
	position: absolute;
	right: 4px;
	display: none;
}
#navtoolbar .bottomtoolbar a:hover{
	background: #0979E9;
}
#navtoolbar .bottomtoolbar a img{
	margin: 1px 0;
}
#navtoolbar .toptoolbar{
	position: fixed;
	right: 0;
	top: 40%;
	width: 30px;
}