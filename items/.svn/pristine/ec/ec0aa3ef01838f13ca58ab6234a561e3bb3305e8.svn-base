}
#footer .footer-logo .logo_tenpay{
	float: left;
	width: 82px;
	height: 40px;
	background: url(../image/logos.png) -83px 0 no-repeat;
}
#footer .footer-logo .logo_unionpay{
	float: left;
	width: 133px;
	height: 40px;
	background: url(../image/logos.png) -165px 0 no-repeat;
}
#footer .footer-logo .logo_ups{
	float: left;
	width: 48px;
	height: 40px;
	background: url(../image/logos.png)0  -40px no-repeat;
}
#footer .footer-logo .logo_fedex{
	float: left;
	width: 72px;
	height: 40px;
	background: url(../image/logos.png) -48px -40px no-repeat;
}
#footer .footer-logo .logo_sfexpress{
	float: left;
	width: 84px;
	height: 40px;
	background: url(../image/logos.png) -120px -40px no-repeat;
}
#footer .footer-logo .logo_gsj{
	float: left;
	width: 84px;
