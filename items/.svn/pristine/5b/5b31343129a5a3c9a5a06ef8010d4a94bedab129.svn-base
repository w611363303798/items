/*banner*/
#banner{
	position: relative;
}
#banner .bannercon .photoList{
	height: 438px;
	position: relative;
}
#banner .bannercon .photoList li{
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 438px;
}
#banner .bannercon .photoList li a{
	width: 100%;
	height: 100%;
	display: inline-block;
}
#banner .pointList{
	position: relative;	
}

#banner .pointList ul{
	width: 78px;
	height: 26px;
	position: absolute;
	left: 50%;
	bottom: 10px;
	margin-left: 260px;
}
#banner .pointList ul li{
	width: 16px;
	height: 16px;
	margin: 5px;
	background: #000;
	opacity: .3;
	float: left;
	border-radius: 50%;
	cursor: pointer;
}
#banner .pointList ul .highlight{
	opacity: 1;
	background: #0979e9;
}
#banner .container{
	height: 438px;
	position: absolute;
	top: 0;
	left: 50%;
	width: 1190px;
	margin-left: -595px;
}
#banner .container .express{
	width: 240px;
	height: 190px;
	position: absolute;
	top: 10px;
	right: 0;
	background: #FDFDFD;
	opacity: .9;
	z-index: 10;
}
#banner .container .express .expressTop{
	width: 240px;
	height: 35px;
	border-bottom: 1px dashed #e4e4e4;
}
#banner .container .express .expressTop h3{
	float: left;
	height: 35px;
	padding-left: 20px;
	line-height: 35px;
	font-weight: 900;
}
#banner .container .express .expressTop h4{
	float: right;
	width: 28px;
	height: 35px;
	padding-right: 20px;
	text-align: center;
	line-height: 35px;
}
#banner .container .express .expressTop h4 a{
	padding: 2px;
	background: #f2f2f2;
	font-size: 12px;
	color: #333;
}
#banner .container .expressCon{
	width: 200px;
	height: 115px;
	padding: 20px;
}
#banner .container .expressCon h5{
	line-height: 14px;
	height: 30px;
	margin-bottom: 20px;
}
#banner .container .expressCon h5 a{
	color: #0979e9;
	font-size: 14px;
}
#banner .container .expressCon p{
	font-size: 12px;
	line-height: 180%;
	color: #666666;
}
#banner .container .gift{
	width: 240px;
	height: 216px;
	position: absolute;
	top: 210px;
	right: 0;
	background: #FDFDFD;
	opacity: 0.9;
	z-index: 10;
}
#banner .container .gift a{
	display: block;
	float: left;
	width: 79px;
	height: 107px;
	text-align: center;
	border-bottom: 1px solid #f2f2f2;
	border-right: 1px solid #f2f2f2;
}
#banner .container .gift a img{
	margin: 20px auto;
	margin-bottom: 0;
}
#banner .container .gift a span{
	font-size: 12px;
	display: block;
	margin-top: 10px;
}
