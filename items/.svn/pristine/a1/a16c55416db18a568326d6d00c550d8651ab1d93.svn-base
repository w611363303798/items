/*container*/
#container{
	width: 1190px;
	margin: 0 auto;
	color: #666;
}
#container a:hover{
	color: #0979E9;
}
#container .cart-title{
	height: 40px;
	width: 100%;
	border-bottom: 2px solid #f2f2f2;
}
#container .cart-title .bar-tabs-cart{
	font-size: 20px;
	position: absolute;
	left: 0;
}
#container .cart-title .bar-tabs-cart li{
	float: left;
}
#container .cart-title .bar-tabs-cart li a{
	border-bottom: 2px solid #0979E9;
	background: #fff;
	color: #0979E9;
	z-index: 50;
	padding: 6px 10px;
	display: block;
	height: 28px;
}
#container .cart-title .carriage{
	position: absolute;
	right: 0;
	top: 10px;
	z-index: 1;
	font-size: 14px;
}
#container .tab-content .tab-pane{
	padding-top: 10px;
}
#container .tab-content .tab-pane table{
	width: 100%;
	font-size: 14px;
	color: #333;
}
#container .tab-content .tab-pane table th,td{
	padding: 8px;
	text-align: left;
}
input[type=checkbox]{
	margin: 0;
	vertical-align: middle;
}
#container .tab-content .tab-pane table tbody input[type=text]{
	line-height: 16px;
	background: #FFF;
	border: 1px solid #e4e4e4;
	text-align: center;
	width: 60px;
}
#container .tab-content .tab-pane table tfoot td{
	border: 1px solid #F2F2F2;
	padding-right: 120px;
}
#container .tab-content .tab-pane table tfoot strong{
	font-weight: 900;
	color: red;
	font-size: 18px;
}
#container table .paybtn{
	position: absolute;
	right: -1px;
	top: -1px;
	height: 35px;
	line-height: 34px;
	font-size: 18px;
	padding: 4px 27px 5px;
	color: #fff;
	background: red;
	border: 1px solid red;
	display: inline-block;
	text-align: center;
}
