		<script type="text/javascript">
			//登录中心显示隐藏
			$(".vip").mouseover(function(){
				$(this).css("background","#fff");
				$(".minicenter").css("display","block")
			})
			$(".vip").mouseout(function(){
				$(this).css("background","");
				$(".minicenter").css("display","none")
			})
			//移除top广告
			$("#adcon > span").click(function(){
				$(this).parent().parent().remove();
			})
			//购物车显示隐藏
			$(".buycarcon").mouseover(function(){
				$(".minicarmenu").css("display","block")
			})
			$(".buycarcon").children().mouseout(function(){
				$(".minicarmenu").css("display","none")
			})
			//二级菜单
			$(".navall").mouseover(function(){
				$(".menunav").css("display","block")
			})
			$(".navall").mouseout(function(){
				$(".menunav").css("display","none")
			})
			$(".menunav ul:first-child").children("li").mouseover(function(){
				$(this).children("div").show().end().find("b").css("color","#0979e9");
				$(this).css("background","#fff").find(".navlink").css("color","#0979e9").end()
				.siblings().css("background","").find(".navlink").css("color","#fff").end();
				$(this).siblings().children("div").hide().parent().find("b").css("color","#fff")
				
			})
			$(".menunav ul:first-child").children("li").mouseout(function(){
				$(this).css("background","").find(".navlink").css("color","#fff");
				$(this).find(".secMenu").hide().parent().find("b").css("color","#fff");
			})
			//回到顶部
			if($(document).scrollTop()>=500){
				$(".bottomtoolbar a").show();
			}else{
				$(".bottomtoolbar a").hide();
			}
			$(".bottomtoolbar a").click(function(){
				$("body,html").animate({scrollTop:0},1000)
			})
			//顶部悬浮
			if($(document).scrollTop()>=120){
				$("#log").css({"position":"fixed","top":"0","zIndex":"1000","height":"92px","paddingTop":"0","borderBottom":"1px solid #ddd"})
			}else{
				$("#log").css({"position":"","zIndex":"","borderBottom":"","height":"102px","paddingTop":"10px","display":"block"})
			}
			//分享
			$(".share").mouseover(function(){
				$(".bsPanel").css("display","block")
			})
			$(".share").mouseout(function(){
				$(".bsPanel").css("display","none")
			})
			//数量增减
			$("#inputNumDown").click(function(){
				var msg = $("#inputNum").val();
				msg--;
				$("#inputNum").val(msg);
				if(parseInt($("#inputNum").val())<parseInt($(".info-items .num").html())){
					$("#counterLimit").css("display","block").html("最小起订为:"+$(".info-items .num").html());
				}else if(parseInt($("#inputNum").val())>parseInt($(".info-items .stress").html())){
					$("#counterLimit").css("display","block").html("库存数:"+$(".info-items .stress").html());
				}else{
					$("#counterLimit").css("display","none")
				}
			})
			$("#inputNumUp").click(function(){
				var msg = $("#inputNum").val();
				msg++;
				$("#inputNum").val(msg);
				if(parseInt($("#inputNum").val())<parseInt($(".info-items .num").html())){
					$("#counterLimit").css("display","block").html("最小起订为:"+$(".info-items .num").html());
				}else if(parseInt($("#inputNum").val())>parseInt($(".info-items .stress").html())){
					$("#counterLimit").css("display","block").html("库存数:"+$(".info-items .stress").html());
				}else{
					$("#counterLimit").css("display","none")
				}
			})
			$("#inputNum").blur(function(){
				if(parseInt($(this).val())<parseInt($(".info-items .num").html())){
					$("#counterLimit").css("display","block").html("最小起订为:"+$(".info-items .num").html());
				}else if(parseInt($(this).val())>parseInt($(".info-items .stress").html())){
					$("#counterLimit").css("display","block").html("库存数:"+$(".info-items .stress").html());
				}else{
					$("#counterLimit").css("display","none")
				}
			})
			//setCookie
			var cookieName="cartCookie";
			var oProduct = {
				name : $(".detail-infos h3").html(),
				maxnum : $(".maxnum").html(),
				minnum : $(".num").html(),
				supplier : $("#suppliers a").html(),
				time : $(".time").html(),
				unitPrice : $("#univalent").html(),
				totalPrices : Number($(".num").html()) * Number($("#univalent").html())
			}
			$(".btn").click(function(){
			var day=new Date();
			day.setDate(day.getDate()+7);
			var obProductsOld={};
			var cookieOld=getCookie(cookieName);
			if(cookieOld.length>0){
				obProductsOld=JSON.parse(cookieOld);
				var flag=false;
				for(var i=0;i<obProductsOld.list.length;i++){
					if(obProductsOld.list[i].name==oProduct.name){
						obProductsOld.list[i].minnum*2;
						flag=true;
					}
				}
				if(!flag){
					obProductsOld.list.push(oProduct);
				}
			}else{
				obProductsOld.list=[];
				obProductsOld.list.push(oProduct);
			}

			var strProduct=JSON.stringify(obProductsOld);
			document.cookie=cookieName+"="+encodeURIComponent(strProduct)+";expires="+day;
			})
			function getCookie(name){
				var value="";
				var strCookie=document.cookie;
				var arrCookie=strCookie.split("; ");
				for(var i=0;i<arrCookie.length;i++){
					var arrItem=arrCookie[i].split("=");
					if(arrItem[0]==name){
						value=arrItem[1];
					}
				}
				return decodeURIComponent(value);
			}
			$("#zoom").elevateZoom({ //鼠标滚动
                scrollZoom: true //是否开启鼠标滚动
            });
		</script>
